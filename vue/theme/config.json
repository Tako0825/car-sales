{
    "global": {
        "$--color-primary": "#A162F7",
        "$--color-success": "#70CF97",
        "$--color-danger": "#FF6370",
        "$--color-info": "#BDC3C7",
        "$--color-warning": "#F6CC0D",
        "$--color-text-primary": "#242731",
        "$--color-text-regular": "#5F6165",
        "$--color-text-secondary": "#FFFFFF",
        "$--color-text-placeholder": "#808191",
        "$--color-black": "#F5F5F5",
        "$--color-white": "#FFFFFF",
        "$--background-color-base": "#F3F5F8",
        "$--border-radius-base": "14px",
        "$--border-radius-small": "6px"
    },
    "local": {
        "$--button-border-radius": "$--border-radius-small",
        "$--button-font-weight": "$--font-weight-primary",
        "$--checkbox-border-radius": "$--border-radius-zero",
        "$--input-background-color": "$--background-color-base",
        "$--input-icon-color": "$--color-text-placeholder",
        "$--input-border-radius": "$--border-radius-small",
        "$--input-font-color": "$--color-text-regular",
        "$--tag-font-size": "14px",
        "$--pagination-font-size": "14px",
        "$--pagination-button-width": "44px",
        "$--pagination-button-height": "32px",
        "$--dialog-padding-primary": "20px",
        "$--dialog-font-line-height": "$--font-line-height-secondary",
        "$--popover-padding": "16px",
        "$--card-padding": "24px",
        "$--carousel-indicator-height": "4px",
        "$--avatar-border-radius": "$--border-radius-small",
        "$--avatar-large-size": "48px",
        "$--avatar-icon-font-size": "28px",
        "$--avatar-background-color": "#F5F5F5",
        "$--avatar-font-color": "#808191",
        "$--message-close-size": "18px"
    }
}